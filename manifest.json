{
  "name": "Slackbot Says",
  "version": "0.2.0",
  "manifest_version": 2,
  "description": "",
  "homepage_url": "https://github.com/smilledge/slackbot-says",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Slackbot says",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://*.slack.com/*",
    "https://*.slack.com/*"
  ]
}
